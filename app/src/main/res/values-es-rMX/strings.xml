<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title for the application. [CHAR LIMIT=12] -->
    <string name="app_name">Just Java</string>

    <string name="txt_quantity">Cantidad</string>
    <string name="txt_zero">2</string>
    <string name="txt_button">ORDEN</string>
    <string name="txt_summary">RESUMEN DE LA ORDEN</string>
    <string name="txt_total">$0</string>
    <string name="plus_sign">+</string>
    <string name="minus_sign">-</string>
    <string name="txt_toppings">Cubiertas</string>
    <string name="txt_checkbox_whipped_cream">Crema Batida</string>
    <string name="txt_checkbox_chocolate">Chocolate</string>
    <string name="hint_name">Nombre</string>

    <!-- Java Strings -->
    <string name="jv_txt_name">Nombre: <xliff:g id="customerName" example="Adan">%s</xliff:g></string>

    <!--
      Whipped cream topping for the order summary. It will be shown in the format of
      "Add whipped cream? true" or "Add whipped cream? false". [CHAR LIMIT=NONE]
    -->
    <string name="jv_txt_add_whipped_cream">Tiene crema batida? <xliff:g id="addWhippedCream" example="true">%b</xliff:g></string>

    <!--
          Chocolate topping for the order summary. It will be shown in the format of
          "Add chocolate? true" or "Add chocolate? false". [CHAR LIMIT=NONE]
        -->
    <string name="jv_txt_add_chocolate">Tiene chocolate?<xliff:g id="addChocolate" example="true">%b</xliff:g></string>

    <!--
      Quantity of coffee cups for the order summary. It will be shown in the format of
      "Quantity: 2", where 2 is the number of cups ordered. [CHAR LIMIT=NONE]
    -->
    <string name="jv_txt_quantity">Cantidad:<xliff:g id="quantity" example="2">%d</xliff:g></string>

    <!--
      Total price for the order summary. It will be shown in the format of
      "Total: $10" where $10 is the price. [CHAR LIMIT=NONE]
    -->
    <string name="jv_txt_total">Total:<xliff:g id="price" example="$10">%s</xliff:g></string>

    <!-- Thank you message for the order summary. [CHAR LIMIT=NONE] -->
    <string name="jv_txt_thank_you">Gracias!</string>

    <!--
      Subject line for the order summary email. It will be in the format of
      "Just Java order for Amy" where Amy is the user's name. [CHAR LIMIT=NONE]
    -->
    <string name="order_summary_email_subject">SÃ³lo Java para <xliff:g id="name" example="Adan">%s</xliff:g></string>
</resources>